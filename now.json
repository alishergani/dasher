{
  "version": 2,
  "alias": ["dasher.sh"],
  "builds": [
    { "src": "frontend/package.json", "use": "@now/next" },
    { "src": "backend/src/index.js", "use": "@now/node-server" }
  ],
  "routes": [
    { "src": "/graphql", "dest": "/backend/src/index.js" },
    { "src": "/(.*)", "dest": "/frontend/$1" }
  ]
}
